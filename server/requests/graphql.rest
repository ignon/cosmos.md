POST http://localhost:4000/
Content-Type: application/json
X-REQUEST-TYPE: GraphQL

mutation addNote($title: String!, $zettelId: String, $text: String!){
  addNote(
    title: $title
    zettelId: $zettelId
    text: $text
  ) {
    title
    zettelId
    text
    tags
    wikilinks { title zettelId }
    # backlinks { title zettelId }
  }
}

